{
  "manifest_version": 2,

  "name": "SS Admin Search Tool ",
  "description": "This extension will help you search the events tab of the admin lookup",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
 ],
 "content_scripts": [{
    "matches": ["https://customersupport.squarespace.com/squarespace-admin/support/*", "https://customercare.squarespace.com/squarespace-admin/support/*"],
    "js":      ["content.js"]
  }]
}
