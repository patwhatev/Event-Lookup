{
  "manifest_version": 2,

  "name": "SS Event Loader",
  "description": "This extension will help you load the entirety of the events tab of the admin lookup with just one click.",
  "version": "1.3",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
 ],
 "content_scripts": [{
    "matches": ["https://customercare.squarespace.com/squarespace-admin/support/*", "https://customersupport.squarespace.com/squarespace-admin/support/*"],
    "js":      ["content.js"]
  }]
}
